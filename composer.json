{
    "name": "designcise/bitframe-geolocation",
	"version": "1.0.0",
    "type": "library",
    "description": "GeoLocation middleware for BitFrame microframework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniyal Hamid",
            "homepage": "https://www.designcise.com/"
        }
    ],
    "require": {
        "php": ">=7.1.0",
		"psr/http-server-middleware": "~1.0",
        "php-http/message": "^1.6",
        "php-http/guzzle6-adapter": "^1.1",
        "willdurand/geocoder": "^4.0",
		"geocoder-php/chain-provider": "^4.0",
        "geocoder-php/free-geoip-provider": "^4.0",
		"geocoder-php/geo-plugin-provider": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "BitFrame\\Locale\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BitFrame\\Test\\": "test/"
        }
    }
}